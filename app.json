{
  "expo": {
    "name": "farmguru",
    "slug": "farmguru",
    "version": "1.0.0",
    "scheme": "farmguru",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-dev-launcher",
        {
          "launchModeExperimental": "most-recent"
        }
      ],

      ["expo-build-properties"]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vais.farmguru"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.vais.farmguru"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5804e828-a85b-4773-87c5-284c7c99f1b6"
      }
    }
  }
}
